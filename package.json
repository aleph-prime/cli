{
  "name": "@aleph-ai/cli",
  "version": "0.1.0",
  "description": "Command-line companion for the Aleph Prime-Resonant Neuro-Symbolic Architecture platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "aleph": "./dist/bin/aleph.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx src/bin/aleph.ts",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "start": "node dist/bin/aleph.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "figures": "^6.1.0",
    "inquirer": "^10.2.2",
    "marked": "^14.1.4",
    "marked-terminal": "^7.2.1",
    "ora": "^8.1.1",
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aleph-prime/cli.git"
  },
  "homepage": "https://aleph.bot",
  "bugs": {
    "url": "https://github.com/aleph-prime/cli"
  },
  "keywords": [
    "aleph",
    "prnsa",
    "prime-resonance",
    "cli",
    "ai",
    "neuro-symbolic",
    "holographic-memory",
    "chat",
    "agents"
  ],
  "author": "Aleph Team <team@aleph.bot>",
  "license": "MIT"
}
